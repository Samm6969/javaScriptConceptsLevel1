 function yearsUntilRetirement(name, year) {
   var age = calculateAge(year);
   var retirementNo = 65 - age;
   console.log(retirementNo);
 }

var name = prompt("enter name plz");
var DOB = prompt("now enter DOB plz");

 yearsUntilRetirement(name, DOB);
